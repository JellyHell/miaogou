.address-manager-cell-container{
  background: #fff;
  display: flex;
  flex-direction: column;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
  border-radius: 10rpx;
}

.user-name{
  font-size: 30rpx;
  color: #1F2D3D;
  margin-top: 30rpx;
  margin-left: 30rpx;
}

.user-address{
  font-size: 26rpx;
  color: #1F2D3D;
  margin-left: 30rpx;
  margin-top: 12rpx;
}

.devider-linner{
  width: 100%;
  height: 1px;
  margin-top: 25rpx;
  background: #f9f9f9;
}

.address-manager-bt-container{
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  flex-direction: row-reverse;
}


.delete-container{
  width: 120rpx;
  height: 50rpx;
  display: flex;
  margin-right: 30rpx;
  border-radius: 8rpx;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border: 0.7px solid #FF4949;
}